	<queryString>
		<![CDATA[SELECT 
	tmsampel.id,
	tmmodel.model,
	tmsampel.nama,
	trsampel_keluar.tgl,
	trsampel_keluar_detail.jumlah,
	tmclient.nama as client,
	tmsampel.unit,
	trsampel_keluar.status_sampel_keluar
FROM trsampel_keluar_detail
JOIN trsampel_keluar ON trsampel_keluar.id = trsampel_keluar_detail.id_sampel_keluar
JOIN tmmodel ON tmmodel.id = trsampel_keluar.id_client
JOIN tmsampel ON tmsampel.id = trsampel_keluar_detail.id_sampel
JOIN tmclient ON tmclient.id = tmsampel.id_client 
WHERE trsampel_keluar.id = $P{id}]]>
